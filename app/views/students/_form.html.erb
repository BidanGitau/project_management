
<div class="row ">

  <div class="col-md-12 m-6 p-6">

    <%= form_with(model: student , class:"row g-3  p-1 mt-3  ") do |form|  %>
    
      <div class="col-md-6 ">

        <%= form.label :name, {class: "form-label"}%>
        <%= form.text_field :name ,{class: "form-control"} %>

      </div>

      <div class="col-md-6">

        <%= form.label :email,{class: "form-label"}%>

        <%=form.email_field :email, class: "form-control"%>

      </div>

      <div class="col-12">

        <%=form.label :password, {class: "form-label"}%>

        <%=form.password_field :password ,{class: "form-control"} %>

      </div>

      <div class="col-md-6">

        <%=form.label :pojectname,{class: "form-label"}%>

        <%=form.text_field :pojectname, {class: "form-control"} %>

      </div>
      <div class="col-md-6">

        <%=form.label :year_of_study, {class: "form-label"}%>

        <%=form.text_field :year_of_study, {class: "form-control"}%>


      </div>

      <div class="col-md-12">

        <%=form.label :instructorname, {class: "form-label"}%>

        <%=form.text_field :instructorname, {class: "form-control"}%>

      </div>

      

      

      <div class="col-12">

        <div class="form-check">

          <input class="form-check-input" type="checkbox" id="gridCheck">

          <label class="form-check-label" for="gridCheck">

          I agree to the terms and condtions 

          </label>

        </div>

      </div>

      <div class="col-md-6">

        <%= form.submit "Submit", class: "btn btn-outline-success" %>

      </div>

      <%end%>
    </div>
  </div>

 
</div>

































